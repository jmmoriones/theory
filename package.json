{
  "name": "theory",
  "version": "0.1.0",
  "description": "Aplicacion creada con Nodejs",
  "scripts": {
    "pug": "gulp pug",
    "sass": "gulp sass",
    "es6": "gulp es6",
    "serve": "browser-sync start --server --startPath \"dist\" --files \"dist/**/*.+(html|css|js)\"",
    "build:dev": "npm run pug && npm run sass && npm run es6",
    "watch:pug": "onchange \"./src/pug/**\" -- npm run pug",
    "watch:sass": "onchange \"./src/scss/**\" -- npm run sass",
    "watch:es6": "onchange \"./src/es6/**\" -- npm run es6",
    "watch:dev": "parallelshell \"npm run serve\" \"npm run watch:pug\" \"npm run watch:sass\" \"npm run watch:es6\"",
    "img": "gulp img",
    "statics": "gulp statics",
    "fonts": "gulp fonts",
    "resize": "mogrify -resize \"1024>\" ./dist/img/*.*",
    "build:media": "npm run img && npm run fonts && npm run statics",
    "css": "gulp css",
    "js": "gulp js",
    "html": "gulp html",
    "build:prod": "npm run css && npm run js && npm run html",
    "watch:css": "onchange \"./dist/css/**\" -- npm run css",
    "watch:js": "onchange \"./dist/js/**\" -- npm run js",
    "watch:html": "onchange \"./dist/*.html\" -- npm run html",
    "watch:prod": "parallelshell \"npm run serve\" \"npm run watch:css\" \"npm run watch:js\" \"npm run watch:html\"",
    "start": "npm run build:prod && npm run watch:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmmoriones/theory.git"
  },
  "author": "JMMoriones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmmoriones/theory/issues"
  },
  "homepage": "https://github.com/jmmoriones/theory#readme",
  "dependencies": {
    "animate.css": "^3.5.2",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "node-sass": "^3.10.0",
    "owl.carousel": "^2.1.4",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.14.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-imagemin": "^3.0.3",
    "gulp-pug": "^3.0.4",
    "gulp-sass": "^2.3.2",
    "gulp-svgmin": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "gulp-uncss": "^1.0.6",
    "gulp-useref": "^3.1.2",
    "imagemin-pngquant": "^5.0.0",
    "onchange": "^3.0.0",
    "parallelshell": "^2.0.0"
  }
}
